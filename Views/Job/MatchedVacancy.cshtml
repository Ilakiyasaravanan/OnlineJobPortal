@model JobPortal.Entity.RecruiterJobDetails

@{
    ViewBag.Title = "MatchedVacancy";
}

<div class="CenterContent">
    <h2 class="heading">These are the matched jobs based on your requirements....</h2>
    <table border="1" class="tableborder">
      
        <tr>
            <th>
                Company Name
            </th>
            <th>
                Graduation
            </th>
            <th>
                Job Type
            </th>
            <th>
                Location
            </th>
            <th>
                Salary
            </th>
            <th>
                Work Experience
            </th>
            <th>
                Action
            </th>

        </tr>
        @foreach (JobPortal.Entity.RecruiterJobDetails item in (List<JobPortal.Entity.RecruiterJobDetails>)ViewData["MatchedJobs"])
        {
            <tr>
                <td>
                    @item.CompanyName
                </td>
                <td>
                    @item.Graduation
                </td>
                <td>
                    @item.Jobtype.JobType
                </td>
                <td>
                    @item.Location
                </td>
                <td>
                    @item.Salary
                </td>
                <td>
                    @item.WorkExperience
                </td>
                <td>
                    @*@Html.ActionLink("Apply", "MatchedVacancy", new { id = item.RecuiterDetailId })*@
                    @Html.ActionLink("Apply", "DisplayCandidateDetails", "Job")
                </td>

            </tr>

        }
    </table>
</div>


